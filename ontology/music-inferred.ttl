#################################################################
#    Classes
#################################################################

###  http://dbpedia.org/resource/Album
<http://dbpedia.org/resource/Album> rdf:type owl:Class ;
                                    owl:equivalentClass music:Album ;
                                    rdfs:subClassOf list:List .


###  http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Bag
bag:Bag rdf:type owl:Class ;
        rdfs:subClassOf collectionentity:Collection ;
        rdfs:comment "Bag - Collection that can have a number of copies of each object"^^xsd:string ;
        rdfs:label "(collections) Bag"^^xsd:string .


###  http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Item
bag:Item rdf:type owl:Class ;
         rdfs:subClassOf owl:Thing ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty bag:itemContent ;
                           owl:cardinality "1"^^xsd:nonNegativeInteger
                         ] ;
         owl:disjointWith collectionentity:Collection ;
         rdfs:comment "Item - Element belonging to a Bag"^^xsd:string ;
         rdfs:label "item"^^xsd:string .


###  http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection
collectionentity:Collection rdf:type owl:Class ;
                            rdfs:comment "Any container for entities that share one or more common properties. E.g. \"stone objects\", \"the nurses\", \"the Louvre Aegyptian collection\". A collection is not a logical class: a collection is a first-order entity, while a class is a second-order one." ;
                            rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl> ;
                            rdfs:label "Collection"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/list.owl#List
list:List rdf:type owl:Class ;
          rdfs:subClassOf bag:Bag ;
          rdfs:comment "List - An ordered array of items, that can be present in multiple copies"^^xsd:string ;
          rdfs:label "list"^^xsd:string .


###  http://www.ontologydesignpatterns.org/cp/owl/list.owl#ListItem
list:ListItem rdf:type owl:Class ;
              rdfs:subClassOf bag:Item ;
              rdfs:comment "ListItem - Element belonging to a list"^^xsd:string ;
              rdfs:label "list item"^^xsd:string .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Album
music:Album rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty bag:hasItem ;
                                  owl:minQualifiedCardinality "6"^^xsd:nonNegativeInteger ;
                                  owl:onClass music:Track
                                ] ;
            rdfs:subClassOf list:List ;
            rdfs:comment "Collection of songs by the same musician/band."@en ,
                         "Raccolta di brani musicali di uno stesso musicista/band."@it ;
            rdfs:label "Album"@en ,
                       "Album"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Artist
music:Artist rdf:type owl:Class ;
             rdfs:subClassOf music:Person ,
                             <http://xmlns.com/foaf/0.1/Person> ;
             rdfs:comment "Person who engages in any form of artistic creation or practice."@en ,
                          "Persona che si impegna in qualsiasi forma di creazione o pratica artistica."@it ;
             rdfs:label "Artist"@en ,
                        "Artista"@it ;
             rdfs:seeAlso "https://www.wikidata.org/wiki/Q483501" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Band
music:Band rdf:type owl:Class ;
           owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                        owl:onProperty collectionentity:hasMember ;
                                                        owl:allValuesFrom music:Musician
                                                      ]
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty collectionentity:hasMember ;
                                                        owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                                        owl:onClass music:Musician
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ;
           rdfs:subClassOf collectionentity:Collection ;
           rdfs:comment "Collettivo di almeno 2 musicisti."@it ,
                        "Group of at least 2 musicians."@en ;
           rdfs:label "Band"@en ,
                      "Band"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#BassPlayer
music:BassPlayer rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Restriction ;
                                       owl:onProperty music:plays ;
                                       owl:someValuesFrom [ owl:intersectionOf ( music:StringedInstrument
                                                                                 [ rdf:type owl:Class ;
                                                                                   owl:oneOf ( music:Bass
                                                                                             )
                                                                                 ]
                                                                               ) ;
                                                            rdf:type owl:Class
                                                          ]
                                     ] ;
                 rdfs:subClassOf music:Musician ;
                 rdfs:comment "Musician playing a stringed instrument, specifically the Bass."@en ,
                              "Musicista che suona uno strumento a corde, nello specifico il Basso."@it ;
                 rdfs:label "Bass Player"@en ,
                            "Bassista"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#BigEvent
music:BigEvent rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty music:attendance ;
                                     owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                          owl:onDatatype xsd:unsignedInt ;
                                                          owl:withRestrictions ( [ xsd:minInclusive "10000"^^xsd:unsignedInt
                                                                                 ]
                                                                               )
                                                        ]
                                   ] ;
               rdfs:subClassOf music:Event ;
               rdfs:comment "Eventi temporanei e programmati, come un festival, un concerto o simili, con almeno 10.000 spettatori."@it ,
                            "Temporary and scheduled events, such as a festival, concert or similar, with at least 10,000 spectators."@en ;
               rdfs:label "Big Event"@en ,
                          "Evento Grande"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Certification
music:Certification rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Restriction ;
                                          owl:onProperty music:certifies ;
                                          owl:someValuesFrom music:Album
                                        ] ;
                    rdfs:comment "Award given to music albums that achieve a certain sales volume."@en ,
                                 "Premio dato agli album musicali che raggiungono un determinato volume di vendite."@it ;
                    rdfs:label "Certification"@en ,
                               "Certificazione"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#City
music:City rdf:type owl:Class ;
           rdfs:subClassOf music:Place ;
           rdfs:comment "Grande insediamento umano permanente."@it ,
                        "Large permanent human settlement."@en ;
           rdfs:label "Città"@it ,
                      "City"@en ;
           rdfs:seeAlso "https://www.wikidata.org/wiki/Q515" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Continent
music:Continent rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Class ;
                                      owl:oneOf ( music:Africa
                                                  music:Asia
                                                  music:Europe
                                                  music:NorthAmerica
                                                  music:Oceania
                                                  music:SouthAmerica
                                                )
                                    ] ;
                rdfs:subClassOf music:Place ;
                rdfs:comment "Grande massa continentale identificata per convenzione."@it ,
                             "Large landmass identified by convention."@en ;
                rdfs:label "Continent"@en ,
                           "Continente"@it ;
                rdfs:seeAlso "https://www.wikidata.org/wiki/Q5107" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#DiamondCertification
music:DiamondCertification rdf:type owl:Class ;
                           rdfs:subClassOf music:Certification ;
                           rdfs:comment "Award given to albums that have sold more than 1,000,000."@en ,
                                        "Premio dato agli album che hanno collezionato un numero di vendite maggiore di 1.000.000."@it ;
                           rdfs:label "Certificazione Diamante"@it ,
                                      "Diamond Certification"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Drummer
music:Drummer rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Restriction ;
                                    owl:onProperty music:plays ;
                                    owl:someValuesFrom [ owl:intersectionOf ( music:PercussionInstrument
                                                                              [ rdf:type owl:Class ;
                                                                                owl:oneOf ( music:Drums
                                                                                          )
                                                                              ]
                                                                            ) ;
                                                         rdf:type owl:Class
                                                       ]
                                  ] ;
              rdfs:subClassOf music:Musician ;
              rdfs:comment "Musician who plays a percussion instrument, specifically the Drums."@en ,
                           "Musicista che suona uno strumento a percussione, nello specifico la Batteria."@it ;
              rdfs:label "Batterista"@it ,
                         "Drummer"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Event
music:Event rdf:type owl:Class ;
            owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                         owl:onProperty music:takePlaceAt ;
                                                         owl:someValuesFrom music:EventLocation
                                                       ]
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty music:attendance ;
                                                         owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                              owl:onDatatype xsd:unsignedInt ;
                                                                              owl:withRestrictions ( [ xsd:minInclusive "1"^^xsd:unsignedInt
                                                                                                     ]
                                                                                                   )
                                                                            ]
                                                       ]
                                                     ) ;
                                  rdf:type owl:Class
                                ] ;
            rdfs:comment "Eventi temporanei e programmati, come un festival, un concerto o simili."@it ,
                         "Temporary and scheduled events, such as a festival, concert or similar."@en ;
            rdfs:label "Event"@en ,
                       "Evento"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#EventLocation
music:EventLocation rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Restriction ;
                                          owl:onProperty music:locatedIn ;
                                          owl:someValuesFrom music:Place
                                        ] ;
                    rdfs:comment "A place where an event is held, such as a sports arena, club, arena, stadium."@en ,
                                 "Posto in cui si tiene un evento, come un palazzetto, un locale, un'arena, uno stadio."@it ;
                    rdfs:label "Location"@en ,
                               "Location"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Genre
music:Genre rdf:type owl:Class ;
            rdfs:comment "Categoria convenzionale che identifica e classifica i brani e le composizioni in base a criteri di affinità."@it ,
                         "Conventional category that identifies and classifies songs and compositions based on criteria of affinity."@en ;
            rdfs:label "Genere Musicale"@it ,
                       "Music Genre"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#GoldCertification
music:GoldCertification rdf:type owl:Class ;
                        rdfs:subClassOf music:Certification ;
                        rdfs:comment "Award given to albums that have had between 50,000 and 100,000 sales."@en ,
                                     "Premio dato agli album che hanno collezionato un numero di vendite compreso tra 50.000 e 100.000."@it ;
                        rdfs:label "Certificazione Oro"@it ,
                                   "Gold Certification"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Guitarist
music:Guitarist rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty music:plays ;
                                      owl:someValuesFrom [ owl:intersectionOf ( music:StringedInstrument
                                                                                [ rdf:type owl:Class ;
                                                                                  owl:oneOf ( music:ClassicGuitar
                                                                                              music:ElectricGuitar
                                                                                              music:RhythmicGuitar
                                                                                            )
                                                                                ]
                                                                              ) ;
                                                           rdf:type owl:Class
                                                         ]
                                    ] ;
                rdfs:subClassOf music:Musician ;
                rdfs:comment "Musician who plays a stringed instrument, specifically the classical guitar or the electric guitar."@en ,
                             "Musicista che suona uno strumento a corde, nello specifico la chitarra classica o la chitarra elettrica."@it ;
                rdfs:label "Chitarrista"@it ,
                           "Guitarist"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Instrument
music:Instrument rdf:type owl:Class ;
                 owl:disjointUnionOf ( music:KeyboardInstrument
                                       music:PercussionInstrument
                                       music:StringedInstrument
                                       music:WindInstrument
                                     ) ;
                 rdfs:comment "Describes all musical instrument."@en ,
                              "Descrive tutti gli strumenti musicali." ;
                 rdfs:label "Instrument"@en ,
                            "Strumento"@it ;
                 rdfs:seeAlso "https://dbpedia.org/ontology/Instrument" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#KeyboardInstrument
music:KeyboardInstrument rdf:type owl:Class ;
                         owl:equivalentClass [ rdf:type owl:Class ;
                                               owl:oneOf ( music:Organ
                                                           music:Pianoforte
                                                           music:Pianola
                                                         )
                                             ] ;
                         rdfs:subClassOf music:Instrument ;
                         rdfs:comment "Tipo di strumento musicale che produce suoni tramite la pressione di tasti o pulsanti, che azionano meccanismi interni che producono il suono."@it ,
                                      "Type of musical instrument that produces sound by pressing keys or buttons, which operate internal mechanisms that produce sound."@en ;
                         rdfs:label "Keyboard Instrument"@en ,
                                    "Strumento a Tastiera"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Keyboardist
music:Keyboardist rdf:type owl:Class ;
                  owl:equivalentClass [ rdf:type owl:Restriction ;
                                        owl:onProperty music:plays ;
                                        owl:someValuesFrom music:KeyboardInstrument
                                      ] ;
                  rdfs:subClassOf music:Musician ;
                  rdfs:comment "Musician who plays electronic keyboards, capable of creating or recreating different types of effects."@en ,
                               "Musicista che suona tastiere elettroniche, in grado di creare o ricreare diversi tipi di effetti."@it ;
                  rdfs:label "Keyboard Player"@en ,
                             "Tastierista"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#MediumEvent
music:MediumEvent rdf:type owl:Class ;
                  owl:equivalentClass [ rdf:type owl:Restriction ;
                                        owl:onProperty music:attendance ;
                                        owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                             owl:onDatatype xsd:unsignedInt ;
                                                             owl:withRestrictions ( [ xsd:minInclusive "5000"^^xsd:unsignedInt
                                                                                    ]
                                                                                    [ xsd:maxExclusive "10000"^^xsd:unsignedInt
                                                                                    ]
                                                                                  )
                                                           ]
                                      ] ;
                  rdfs:subClassOf music:Event ;
                  rdfs:comment "Eventi temporanei e programmati, come un festival, un concerto o simili, con un numero di spettatori compreso tra 5.000 e 10.000."@it ,
                               "Temporary and scheduled events, such as a festival, concert or similar, with an audience of between 5,000 and 10,000."@en ;
                  rdfs:label "Evento Medio"@it ,
                             "Medium Event"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Musician
music:Musician rdf:type owl:Class ;
               owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Class ;
                                                            owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                            owl:onProperty music:plays ;
                                                                            owl:someValuesFrom [ rdf:type owl:Class ;
                                                                                                 owl:oneOf ( music:Voice
                                                                                                           )
                                                                                               ]
                                                                          ]
                                                                          [ rdf:type owl:Restriction ;
                                                                            owl:onProperty music:plays ;
                                                                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                            owl:onClass music:Instrument
                                                                          ]
                                                                        )
                                                          ]
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty music:hasWritten ;
                                                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                            owl:onClass music:Song
                                                          ]
                                                        ) ;
                                     rdf:type owl:Class
