@prefix : <http://www.semanticweb.org/lorenzo/ontologies/music> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix bag: <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix list: <http://www.ontologydesignpatterns.org/cp/owl/list.owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix swrl: <http://www.w3.org/2003/11/swrl#> .
@prefix music: <http://www.semanticweb.org/lorenzo/ontologies/music#> .
@prefix swrla: <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#> .
@prefix swrlb: <http://www.w3.org/2003/11/swrlb#> .
@prefix cpannotationschema: <http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#> .
@base <http://www.semanticweb.org/lorenzo/ontologies/music> .

<http://www.semanticweb.org/lorenzo/ontologies/music> rdf:type owl:Ontology ;
                                                       owl:imports <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl> ,
                                                                   <http://www.ontologydesignpatterns.org/cp/owl/list.owl> ;
                                                       dc:creator "Lorenzo Favaro" ;
                                                       dc:date "2023-01-25" ;
                                                       dc:description "Questa ontologia descrive il mondo musicale, rivolgendosi agli eventi musicali con particolare enfasi."@it ,
                                                                      "This ontology describes the musical world, addressing musical events with special emphasis."@en ;
                                                       dc:title "Mondo Musicale & Eventi"@it ,
                                                                "Music & Events World"@en ;
                                                       music:preferredNamespacePrefix "music" ;
                                                       music:preferredNamespaceUri "http://www.semanticweb.org/lorenzo/ontologies/music#" ;
                                                       rdfs:label "Music & Events"@en ,
                                                                  "Musica ed Eventi"@it .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
dc:date rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
swrla:isRuleEnabled rdf:type owl:AnnotationProperty .


###  http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem
bag:hasItem rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/lorenzo/ontologies/music#preferredNamespacePrefix
music:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/lorenzo/ontologies/music#preferredNamespaceUri
music:preferredNamespaceUri rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#sameAs
owl:sameAs rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember
<http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember> owl:inverseOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf> ;
                                                                              rdfs:label "ha membro"@it .


###  http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf
<http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf> rdfs:label "membro di"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#albumHasGenre
music:albumHasGenre rdf:type owl:ObjectProperty ;
                    rdfs:domain music:Album ;
                    rdfs:range music:Genre ;
                    rdfs:comment "album ha genere musicale"@it ,
                                 "album has musical genre"@en ;
                    rdfs:label "album ha genere"@it ,
                               "album has genre"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#albumOf
music:albumOf rdf:type owl:ObjectProperty ;
              owl:inverseOf music:hasAlbum ;
              rdfs:domain music:Album ;
              rdfs:range [ rdf:type owl:Class ;
                           owl:unionOf ( music:Band
                                         music:Musician
                                       )
                         ] ;
              rdfs:comment "album appartiene a musicista/band"@it ,
                           "album belongs to musician/band"@en ;
              rdfs:label "album di"@it ,
                         "album of"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#belongsToEvent
music:belongsToEvent rdf:type owl:ObjectProperty ;
                     owl:inverseOf music:hasPerformance ;
                     rdf:type owl:FunctionalProperty ;
                     rdfs:domain music:Performance ;
                     rdfs:range music:Event ;
                     rdfs:comment "la performance appartiene all'evento"@it ,
                                  "the performance belongs to the event"@en ;
                     rdfs:label "appartiene a evento"@it ,
                                "belongs to event"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#certifies
music:certifies rdf:type owl:ObjectProperty ;
                owl:inverseOf music:hasCertification ;
                rdfs:domain music:Certification ;
                rdfs:range music:Album ;
                rdfs:comment "certification certifies album"@en ,
                             "la certificazione certifica album"@it ;
                rdfs:label "certifica"@it ,
                           "certifies"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#componentOf
music:componentOf rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf> ;
                  owl:inverseOf music:hasComponent ;
                  rdfs:domain music:Musician ;
                  rdfs:range music:Band ;
                  rdfs:comment "il musicista è membro della band"@it ,
                               "the musician is a member of the band"@en ;
                  rdfs:label "component of"@en ,
                             "componente di"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#containsSong
music:containsSong rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf bag:itemContent ;
                   rdfs:domain music:Track ;
                   rdfs:range music:Song ;
                   rdfs:comment "la traccia contiene il brano"@it ,
                                "the track contains the song"@en ;
                   rdfs:label "contains song"@en ,
                              "contiene brano"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#doPerform
music:doPerform rdf:type owl:ObjectProperty ;
                owl:inverseOf music:performanceBy ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( music:Band
                                            music:Musician
                                          )
                            ] ;
                rdfs:range music:Performance ;
                rdfs:comment "band/musician does the performance"@en ,
                             "band/musicista compie la performance"@it ;
                rdfs:label "do perform"@en ,
                           "performa"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#eventHasTag
music:eventHasTag rdf:type owl:ObjectProperty ;
                  rdfs:domain music:Event ;
                  rdfs:range music:Genre ;
                  owl:propertyChainAxiom ( music:hasPerformance
                                           music:hasSongPerformed
                                           music:hasGenre
                                         ) ;
                  rdfs:comment "l'evento ha il genere (tag)"@it ,
                               "the event has the genre (tag)"@en ;
                  rdfs:label "event has tag"@en ,
                             "evento ha tag"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#genreOfSong
music:genreOfSong rdf:type owl:ObjectProperty ;
                  owl:inverseOf music:hasGenre ;
                  rdfs:domain music:Genre ;
                  rdfs:range music:Song ;
                  rdfs:comment "is the genre of the song"@en ,
                               "è il genere del brano"@it ;
                  rdfs:label "genere del brano"@it ,
                             "genre of song"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#hasAlbum
music:hasAlbum rdf:type owl:ObjectProperty ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( music:Band
                                           music:Musician
                                         )
                           ] ;
               rdfs:range music:Album ;
               rdfs:comment "band/musician has the album"@en ,
                            "band/musicista ha l'album"@it ;
               rdfs:label "ha album"@it ,
                          "has album"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#hasCertification
music:hasCertification rdf:type owl:ObjectProperty ;
                       rdfs:domain music:Album ;
                       rdfs:range music:Certification ;
                       rdfs:comment "l'album ha certificazione"@it ,
                                    "the album has certification"@en ;
                       rdfs:label "ha certificazione"@it ,
                                  "has certification"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#hasComponent
music:hasComponent rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember> ;
                   rdfs:domain music:Band ;
                   rdfs:range music:Musician ;
                   rdfs:comment "la band ha componente"@it ,
                                "the band has member"@en ;
                   rdfs:label "ha componente"@it ,
                              "has component"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#hasEventPlaced
music:hasEventPlaced rdf:type owl:ObjectProperty ;
                     owl:inverseOf music:takePlaceAt ;
                     rdfs:domain music:EventLocation ;
                     rdfs:range music:Event ;
                     rdfs:comment "la location ospita l'evento"@it ,
                                  "the location hosts the event"@en ;
                     rdfs:label "hosts event"@en ,
                                "ospita evento"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#hasGenre
music:hasGenre rdf:type owl:ObjectProperty ;
               rdfs:domain music:Song ;
               rdfs:range music:Genre ;
               rdfs:comment "il brano ha genere musicale"@it ,
                            "the song has musical genre"@en ;
               rdfs:label "ha genere"@it ,
                          "has genre"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#hasLocation
music:hasLocation rdf:type owl:ObjectProperty ;
                  owl:inverseOf music:locatedIn ;
                  rdfs:domain music:Place ;
                  rdfs:range music:EventLocation ;
                  rdfs:comment "il luogo contiene la location"@it ,
                               "the place contains the location"@en ;
                  rdfs:label "contiene location"@it ,
                             "has location"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#hasPerformance
music:hasPerformance rdf:type owl:ObjectProperty ;
                     rdfs:domain music:Event ;
                     rdfs:range music:Performance ;
                     rdfs:comment "l'evento ha performance"@it ,
                                  "the event has performance"@en ;
                     rdfs:label "ha performance"@it ,
                                "has performance"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#hasSongPerformed
music:hasSongPerformed rdf:type owl:ObjectProperty ;
                       owl:inverseOf music:performedIn ;
                       rdfs:domain music:Performance ;
                       rdfs:range music:Song ;
                       rdfs:comment "la performance riguarda il brano"@it ,
                                    "the performance is about the song"@en ;
                       rdfs:label "ha brano performato"@it ,
                                  "has song performed"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#hasWritten
music:hasWritten rdf:type owl:ObjectProperty ;
                 owl:inverseOf music:writtenBy ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( music:Band
                                             music:Musician
                                           )
                             ] ;
                 rdfs:range music:Song ;
                 rdfs:comment "band/musician wrote the song"@en ,
                              "band/musicista ha scritto il brano"@it ;
                 rdfs:label "ha scritto"@it ,
                            "has written"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#hostOf
music:hostOf rdf:type owl:ObjectProperty ;
             owl:inverseOf music:hosts ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( music:Band
                                         music:Musician
                                       )
                         ] ;
             rdfs:range music:Event ;
             rdfs:comment "band/musician is guest of the event"@en ,
                          "band/musicista è ospite dell'evento"@it ;
             rdfs:label "host of"@en ,
                        "ospite di"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#hosts
music:hosts rdf:type owl:ObjectProperty ;
            rdfs:domain music:Event ;
            rdfs:range [ rdf:type owl:Class ;
                         owl:unionOf ( music:Band
                                       music:Musician
                                     )
                       ] ;
            owl:propertyChainAxiom ( music:hasPerformance
                                     music:performanceBy
                                   ) ;
            rdfs:comment "event x (which has performance y which is performed by band/musician z) hosts band/musician"@en ,
                         "l'evento x (che ha performance y che è performata da band/musicista z) ospita band/musicista"@it ;
            rdfs:label "hosts"@en ,
                       "ospita"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#locatedIn
music:locatedIn rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain music:EventLocation ;
                rdfs:range music:Place ;
                rdfs:comment "la location è situata nel luogo"@it ,
                             "the location is located in the place"@en ;
                rdfs:label "located in"@en ,
                           "situata in"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#performanceBy
music:performanceBy rdf:type owl:ObjectProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain music:Performance ;
                    rdfs:range [ rdf:type owl:Class ;
                                 owl:unionOf ( music:Band
                                               music:Musician
                                             )
                               ] ;
                    rdfs:comment "la performance appartiene a band/musicista"@it ,
                                 "performance belongs to band/musician"@en ;
                    rdfs:label "performance by"@en ,
                               "performance di"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#performedIn
music:performedIn rdf:type owl:ObjectProperty ;
                  rdfs:domain music:Song ;
                  rdfs:range music:Performance ;
                  rdfs:comment "il brano è performato nella performance"@it ,
                               "the song is performed in the performance"@en ;
                  rdfs:label "performato in"@it ,
                             "performed in"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#playedBy
music:playedBy rdf:type owl:ObjectProperty ;
               owl:inverseOf music:plays ;
               rdfs:domain music:Instrument ;
               rdfs:range music:Musician ;
               rdfs:comment "lo strumento è suonato dal musicista"@it ,
                            "the instrument is played by the musician"@en ;
               rdfs:label "played by"@en ,
                          "suonato da"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#plays
music:plays rdf:type owl:ObjectProperty ;
            rdfs:domain music:Musician ;
            rdfs:range music:Instrument ;
            rdfs:comment "il musicista suona lo strumento"@it ,
                         "the musician plays the instrument"@en ;
            rdfs:label "plays"@en ,
                       "suona"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#positionedIn
music:positionedIn rdf:type owl:ObjectProperty ,
                            owl:TransitiveProperty ;
                   rdfs:domain music:Place ;
                   rdfs:range music:Place ;
                   rdfs:comment "il luogo x è collocato all'interno del luogo y"@it ,
                                "place x is located inside place y"@en ;
                   rdfs:label "collocato in"@it ,
                              "positioned in"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#takePlaceAt
music:takePlaceAt rdf:type owl:ObjectProperty ;
                  rdfs:domain music:Event ;
                  rdfs:range music:EventLocation ;
                  rdfs:comment "l'evento è tenuto presso la location"@it ,
                               "the event takes place at the location"@en ;
                  rdfs:label "takes place at"@en ,
                             "tenuto presso"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#writtenBy
music:writtenBy rdf:type owl:ObjectProperty ;
                rdfs:domain music:Song ;
                rdfs:range [ rdf:type owl:Class ;
                             owl:unionOf ( music:Band
                                           music:Musician
                                         )
                           ] ;
                rdfs:comment "il brano è scritto da band/musicista"@it ,
                             "the song is written by the band/musician"@en ;
                rdfs:label "scritto da"@it ,
                           "written by"@en .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/lorenzo/ontologies/music#attendance
music:attendance rdf:type owl:DatatypeProperty ;
                 rdfs:domain music:Event ;
                 rdfs:range xsd:unsignedInt ;
                 rdfs:comment "number of spectators attending an event"@en ,
                              "numero degli spettatori che assistono ad un evento"@it ;
                 rdfs:label "attendance"@en ,
                            "numero spettatori"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#birthDate
music:birthDate rdf:type owl:DatatypeProperty ;
                rdfs:domain music:Person ;
                rdfs:range xsd:date ;
                rdfs:comment "data di nascita di una persona"@it ,
                             "date of birth of a person"@en ;
                rdfs:label "birth date"@en ,
                           "data di nascita"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#eventEndDate
music:eventEndDate rdf:type owl:DatatypeProperty ;
                   rdfs:domain music:Event ;
                   rdfs:range xsd:date ;
                   rdfs:comment "data in cui termina un evento"@it ,
                                "date on which an event ends"@en ;
                   rdfs:label "data fine evento"@it ,
                              "event end date"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#eventStartDate
music:eventStartDate rdf:type owl:DatatypeProperty ;
                     rdfs:domain music:Event ;
                     rdfs:range xsd:date ;
                     rdfs:comment "data in cui inizia un evento"@it ,
                                  "date on which an event begins"@en ;
                     rdfs:label "data inizio evento"@it ,
                                "event start date"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#eventType
music:eventType rdf:type owl:DatatypeProperty ;
                rdfs:domain music:Event ;
                rdfs:range xsd:string ;
                rdfs:comment "tipologia dell'evento"@it ,
                             "type of event"@en ;
                rdfs:label "event type"@en ,
                           "tipo evento"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#manifest
music:manifest rdf:type owl:DatatypeProperty ;
               rdfs:domain music:Event ;
               rdfs:range xsd:anyURI ;
               rdfs:comment "URI containing the event poster image"@en ,
                            "URI contenente l'immagine di manifesto dell'evento"@it ;
               rdfs:label "manifest"@en ,
                          "manifesto"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#releaseDate
music:releaseDate rdf:type owl:DatatypeProperty ;
                  rdfs:domain music:Album ;
                  rdfs:range xsd:date ;
                  rdfs:comment "data di rilascio di un album musicale"@it ,
                               "release date of a music album"@en ;
                  rdfs:label "data uscita"@it ,
                             "release date"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#salesCount
music:salesCount rdf:type owl:DatatypeProperty ;
                 rdfs:domain music:Album ;
                 rdfs:range xsd:unsignedInt ;
                 rdfs:comment "number of album sales"@en ,
                              "number of album sales"@it ;
                 rdfs:label "numero vendite"@it ,
                            "sales count"@en .


#################################################################
#    Classes
#################################################################

###  http://dbpedia.org/resource/Album
<http://dbpedia.org/resource/Album> rdf:type owl:Class ;
                                    owl:equivalentClass music:Album .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Album
music:Album rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty bag:hasItem ;
                                  owl:minQualifiedCardinality "6"^^xsd:nonNegativeInteger ;
                                  owl:onClass music:Track
                                ] ;
            rdfs:subClassOf list:List ;
            rdfs:comment "Collection of songs by the same musician/band."@en ,
                         "Raccolta di brani musicali di uno stesso musicista/band."@it ;
            rdfs:label "Album"@en ,
                       "Album"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Artist
music:Artist rdf:type owl:Class ;
             rdfs:subClassOf music:Person ;
             rdfs:comment "Person who engages in any form of artistic creation or practice."@en ,
                          "Persona che si impegna in qualsiasi forma di creazione o pratica artistica."@it ;
             rdfs:label "Artist"@en ,
                        "Artista"@it ;
             rdfs:seeAlso "https://www.wikidata.org/wiki/Q483501" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Band
music:Band rdf:type owl:Class ;
           owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                        owl:onProperty music:hasComponent ;
                                                        owl:allValuesFrom music:Musician
                                                      ]
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty music:hasComponent ;
                                                        owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                                        owl:onClass music:Musician
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ;
           rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection> ;
           rdfs:comment "Collettivo di almeno 2 musicisti."@it ,
                        "Group of at least 2 musicians."@en ;
           rdfs:label "Band"@en ,
                      "Band"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#BassPlayer
music:BassPlayer rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Restriction ;
                                       owl:onProperty music:plays ;
                                       owl:someValuesFrom [ owl:intersectionOf ( music:StringedInstrument
                                                                                 [ rdf:type owl:Class ;
                                                                                   owl:oneOf ( music:Bass
                                                                                             )
                                                                                 ]
                                                                               ) ;
                                                            rdf:type owl:Class
                                                          ]
                                     ] ;
                 rdfs:subClassOf music:Musician ;
                 rdfs:comment "Musician playing a stringed instrument, specifically the Bass."@en ,
                              "Musicista che suona uno strumento a corde, nello specifico il Basso."@it ;
                 rdfs:label "Bass Player"@en ,
                            "Bassista"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#BigEvent
music:BigEvent rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty music:attendance ;
                                     owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                          owl:onDatatype xsd:unsignedInt ;
                                                          owl:withRestrictions ( [ xsd:minInclusive "10000"^^xsd:unsignedInt
                                                                                 ]
                                                                               )
                                                        ]
                                   ] ;
               rdfs:subClassOf music:Event ;
               rdfs:comment "Eventi temporanei e programmati, come un festival, un concerto o simili, con almeno 10.000 spettatori."@it ,
                            "Temporary and scheduled events, such as a festival, concert or similar, with at least 10,000 spectators."@en ;
               rdfs:label "Big Event"@en ,
                          "Evento Grande"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Certification
music:Certification rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Restriction ;
                                          owl:onProperty music:certifies ;
                                          owl:someValuesFrom music:Album
                                        ] ;
                    rdfs:comment "Award given to music albums that achieve a certain sales volume."@en ,
                                 "Premio dato agli album musicali che raggiungono un determinato volume di vendite."@it ;
                    rdfs:label "Certification"@en ,
                               "Certificazione"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#City
music:City rdf:type owl:Class ;
           rdfs:subClassOf music:Place ;
           rdfs:comment "Grande insediamento umano permanente."@it ,
                        "Large permanent human settlement."@en ;
           rdfs:label "Città"@it ,
                      "City"@en ;
           rdfs:seeAlso "https://www.wikidata.org/wiki/Q515" .


