@prefix : <http://www.semanticweb.org/lorenzo/ontologies/music#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix musica: <http://www.semanticweb.org/lorenzo/ontologies/music#> .
@base <http://www.semanticweb.org/lorenzo/ontologies/music> .

<http://www.semanticweb.org/lorenzo/ontologies/music> rdf:type owl:Ontology ;
                                                       owl:imports <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl> .

#################################################################
#    Object Properties
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember
<http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember> owl:inverseOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf> ;
                                                                              rdfs:label "ha membro" .


###  http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf
<http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf> rdfs:label "membro di" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#albumHaGenere
musica:albumHaGenere rdf:type owl:ObjectProperty ;
                     rdfs:domain musica:Album ;
                     rdfs:range musica:GenereMusicale ;
                     owl:propertyChainAxiom ( musica:contiene
                                              musica:haGenere
                                            ) ;
                     rdfs:label "album ha genere" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#assisteA
musica:assisteA rdf:type owl:ObjectProperty ;
                rdfs:domain musica:Persona ;
                rdfs:range musica:Evento ;
                rdfs:label "assiste a" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#componenteDi
musica:componenteDi rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf> ;
                    owl:inverseOf musica:haComponente ;
                    rdfs:domain musica:Musicista ;
                    rdfs:range musica:Band ;
                    rdfs:label "componente di" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#contenutoIn
musica:contenutoIn rdf:type owl:ObjectProperty ;
                   owl:inverseOf musica:contiene ;
                   rdf:type owl:FunctionalProperty ;
                   rdfs:domain musica:Brano ;
                   rdfs:range musica:Album ;
                   rdfs:label "contenuto in" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#contiene
musica:contiene rdf:type owl:ObjectProperty ;
                rdfs:domain musica:Album ;
                rdfs:range musica:Brano ;
                rdfs:label "contiene" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#distribuisce
musica:distribuisce rdf:type owl:ObjectProperty ;
                    owl:inverseOf musica:distribuitoTramite ;
                    rdfs:domain musica:CanaleDistribuzione ;
                    rdfs:range [ rdf:type owl:Class ;
                                 owl:unionOf ( musica:Album
                                               musica:Brano
                                             )
                               ] ;
                    rdfs:label "distribuisce" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#distribuitoTramite
musica:distribuitoTramite rdf:type owl:ObjectProperty ;
                          rdfs:domain [ rdf:type owl:Class ;
                                        owl:unionOf ( musica:Album
                                                      musica:Brano
                                                    )
                                      ] ;
                          rdfs:range musica:CanaleDistribuzione ;
                          rdfs:label "distribuito tramite" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#genereDelBrano
musica:genereDelBrano rdf:type owl:ObjectProperty ;
                      owl:inverseOf musica:haGenere ;
                      rdfs:domain musica:GenereMusicale ;
                      rdfs:range musica:Brano ;
                      rdfs:label "genere di" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#haAlbum
musica:haAlbum rdf:type owl:ObjectProperty ;
               rdfs:domain musica:Musicista ;
               rdfs:range musica:Album ;
               owl:propertyChainAxiom ( musica:haScritto
                                        musica:contenutoIn
                                      ) ;
               rdfs:label "ha album" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#haComponente
musica:haComponente rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember> ;
                    rdfs:domain musica:Band ;
                    rdfs:range musica:Musicista ;
                    rdfs:label "ha componente" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#haGenere
musica:haGenere rdf:type owl:ObjectProperty ;
                rdfs:domain musica:Brano ;
                rdfs:range musica:GenereMusicale ;
                rdfs:label "ha genere" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#haIngaggiato
musica:haIngaggiato rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember> ;
                    owl:inverseOf musica:ingaggiatoDa ;
                    rdfs:domain musica:CasaDiscografica ;
                    rdfs:range [ rdf:type owl:Class ;
                                 owl:unionOf ( musica:Musicista
                                               musica:Produttore
                                             )
                               ] ;
                    rdfs:label "ha ingaggiato" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#haProdotto
musica:haProdotto rdf:type owl:ObjectProperty ;
                  owl:inverseOf musica:prodottoDa ;
                  rdfs:domain musica:Produttore ;
                  rdfs:range musica:Brano ;
                  rdfs:label "ha prodotto" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#haScritto
musica:haScritto rdf:type owl:ObjectProperty ;
                 owl:inverseOf musica:scrittoDa ;
                 rdfs:domain musica:Musicista ;
                 rdfs:range musica:Brano ;
                 rdfs:label "ha scritto" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#ingaggiatoDa
musica:ingaggiatoDa rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf> ;
                    rdfs:domain [ rdf:type owl:Class ;
                                  owl:unionOf ( musica:Musicista
                                                musica:Produttore
                                              )
                                ] ;
                    rdfs:range musica:CasaDiscografica ;
                    rdfs:label "ingaggiato da" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#ospita
musica:ospita rdf:type owl:ObjectProperty ;
              owl:inverseOf musica:ospiteDi ;
              rdfs:domain musica:Evento ;
              rdfs:range [ rdf:type owl:Class ;
                           owl:unionOf ( musica:Band
                                         musica:Musicista
                                       )
                         ] ;
              rdfs:label "ospita" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#ospiteDi
musica:ospiteDi rdf:type owl:ObjectProperty ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( musica:Band
                                            musica:Musicista
                                          )
                            ] ;
                rdfs:range musica:Evento ;
                rdfs:label "ospite di" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#prodottoDa
musica:prodottoDa rdf:type owl:ObjectProperty ;
                  rdfs:domain musica:Brano ;
                  rdfs:range musica:Produttore ;
                  rdfs:label "prodotto da" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#scrittoDa
musica:scrittoDa rdf:type owl:ObjectProperty ;
                 rdfs:domain musica:Brano ;
                 rdfs:range musica:Musicista ;
                 rdfs:label "scritto da" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#situatoIn
musica:situatoIn rdf:type owl:ObjectProperty ,
                          owl:TransitiveProperty ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( musica:Evento
                                             musica:Luogo
                                           )
                             ] ;
                 rdfs:range musica:Luogo ;
                 rdfs:label "situato in" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#suona
musica:suona rdf:type owl:ObjectProperty ;
             owl:inverseOf musica:suonatoDa ;
             rdfs:domain musica:Musicista ;
             rdfs:range musica:Strumento ;
             rdfs:label "suona" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#suonatoDa
musica:suonatoDa rdf:type owl:ObjectProperty ;
                 rdfs:domain musica:Strumento ;
                 rdfs:range musica:Musicista ;
                 rdfs:label "suonato da" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#tag
musica:tag rdf:type owl:ObjectProperty ;
           rdfs:domain musica:Musicista ;
           rdfs:range musica:GenereMusicale ;
           owl:propertyChainAxiom ( musica:haScritto
                                    musica:haGenere
                                  ) .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/lorenzo/ontologies/music#dataEvento
musica:dataEvento rdf:type owl:DatatypeProperty ;
                  rdfs:domain musica:Evento ;
                  rdfs:range xsd:dateTime ;
                  rdfs:label "data evento" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#dataUscita
musica:dataUscita rdf:type owl:DatatypeProperty ;
                  rdfs:domain musica:Album ;
                  rdfs:range xsd:dateTime ;
                  rdfs:label "data uscita" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#età
musica:età rdf:type owl:DatatypeProperty ;
           rdfs:domain musica:Persona ;
           rdfs:range xsd:unsignedInt ;
           rdfs:label "età" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#numeroSpettatori
musica:numeroSpettatori rdf:type owl:DatatypeProperty ;
                        rdfs:domain musica:Evento ;
                        rdfs:range xsd:unsignedInt ;
                        rdfs:label "numero spettatori" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#numeroVendite
musica:numeroVendite rdf:type owl:DatatypeProperty ;
                     rdfs:domain musica:Album ;
                     rdfs:range xsd:unsignedInt ;
                     rdfs:label "numero vendite" .


#################################################################
#    Classes
#################################################################

###  http://dbpedia.org/resource/Album
<http://dbpedia.org/resource/Album> rdf:type owl:Class ;
                                    owl:equivalentClass musica:Album .


###  http://dbpedia.org/resource/Shop
<http://dbpedia.org/resource/Shop> rdf:type owl:Class ;
                                   owl:equivalentClass musica:Negozio ;
                                   rdfs:subClassOf musica:CanaleFisico .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Adulto
musica:Adulto rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Restriction ;
                                    owl:onProperty musica:età ;
                                    owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                         owl:onDatatype xsd:unsignedInt ;
                                                         owl:withRestrictions ( [ xsd:minInclusive "30"^^xsd:unsignedInt
                                                                                ]
                                                                                [ xsd:maxExclusive "65"^^xsd:unsignedInt
                                                                                ]
                                                                              )
                                                       ]
                                  ] ;
              rdfs:subClassOf musica:Generazione ;
              rdfs:comment "Persona che ha raggiunto il pieno sviluppo fisico e psichico di età compresa tra 30 e 65 anni." ;
              rdfs:label "Adult"@en ,
                         "Adulto"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Album
musica:Album rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Restriction ;
                                   owl:onProperty musica:contiene ;
                                   owl:minQualifiedCardinality "3"^^xsd:nonNegativeInteger ;
                                   owl:onClass musica:Brano
                                 ] ;
             rdfs:comment "Raccolta di brani musicali di uno stesso artista." ;
             rdfs:label "Album"@en ,
                        "Album"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#AlbumDiamante
musica:AlbumDiamante rdf:type owl:Class ;
                     owl:equivalentClass [ rdf:type owl:Restriction ;
                                           owl:onProperty musica:numeroVendite ;
                                           owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                owl:onDatatype xsd:unsignedInt ;
                                                                owl:withRestrictions ( [ xsd:minInclusive "1000000"^^xsd:unsignedInt
                                                                                       ]
                                                                                     )
                                                              ]
                                         ] ;
                     rdfs:subClassOf musica:Album ;
                     rdfs:comment "Album musicale che ha totalizzato un numero di vendite pari ad almeno 1 milione." ;
                     rdfs:label "Album di Diamante"@it ,
                                "Diamant Album"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#AlbumOro
musica:AlbumOro rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty musica:numeroVendite ;
                                      owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                           owl:onDatatype xsd:unsignedInt ;
                                                           owl:withRestrictions ( [ xsd:minExclusive "50000"^^xsd:unsignedInt
                                                                                  ]
                                                                                  [ xsd:maxExclusive "100000"^^xsd:unsignedInt
                                                                                  ]
                                                                                )
                                                         ]
                                    ] ;
                rdfs:subClassOf musica:Album ;
                rdfs:comment "Album musicale che ha totalizzato un numero di vendite compreso tra 50 mila e 100 mila." ;
                rdfs:label "Album d'Oro"@it ,
                           "Gold Album"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#AlbumPlatino
musica:AlbumPlatino rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Restriction ;
                                          owl:onProperty musica:numeroVendite ;
                                          owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                               owl:onDatatype xsd:unsignedInt ;
                                                               owl:withRestrictions ( [ xsd:minExclusive "100000"^^xsd:unsignedInt
                                                                                      ]
                                                                                      [ xsd:maxExclusive "1000000"^^xsd:unsignedInt
                                                                                      ]
                                                                                    )
                                                             ]
                                        ] ;
                    rdfs:subClassOf musica:Album ;
                    rdfs:comment "Album musicale che ha totalizzato un numero di vendite compreso tra 100 mila e 1 milione." ;
                    rdfs:label "Album di Platino"@it ,
                               "Platino Album"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Anziano
musica:Anziano rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty musica:età ;
                                     owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                          owl:onDatatype xsd:unsignedInt ;
                                                          owl:withRestrictions ( [ xsd:minInclusive "65"^^xsd:unsignedInt
                                                                                 ]
                                                                               )
                                                        ]
                                   ] ;
               rdfs:subClassOf musica:Generazione ;
               rdfs:comment "Persona di età uguale o superiore a 65 anni." ;
               rdfs:label "Anziano"@it ,
                          "Elderly"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Artista
musica:Artista rdf:type owl:Class ;
               rdfs:subClassOf musica:Persona ;
               rdfs:label "Artist"@en ,
                          "Artista"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Band
musica:Band rdf:type owl:Class ;
            owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                         owl:onProperty <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember> ;
                                                         owl:allValuesFrom musica:Musicista
                                                       ]
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember> ;
                                                         owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                         owl:onClass musica:Musicista
                                                       ]
                                                     ) ;
                                  rdf:type owl:Class
                                ] ;
            rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection> ;
            owl:disjointWith musica:CasaDiscografica ;
            rdfs:comment "Gruppo di musicisti." ;
            rdfs:label "Band"@en ,
                       "Band"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Bassista
musica:Bassista rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty musica:suona ;
                                      owl:someValuesFrom [ owl:intersectionOf ( musica:StrumentoACorde
                                                                                [ rdf:type owl:Class ;
                                                                                  owl:oneOf ( musica:Basso
                                                                                            )
                                                                                ]
                                                                              ) ;
                                                           rdf:type owl:Class
                                                         ]
                                    ] ;
                rdfs:subClassOf musica:Musicista ;
                rdfs:comment "Musicista che suona uno strumento a corde, nello specifico il Basso." ;
                rdfs:label "Bass Player"@en ,
                           "Bassista"@it .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Batterista
musica:Batterista rdf:type owl:Class ;
                  owl:equivalentClass [ rdf:type owl:Restriction ;
                                        owl:onProperty musica:suona ;
                                        owl:someValuesFrom [ owl:intersectionOf ( musica:StrumentoAPercussione
                                                                                  [ rdf:type owl:Class ;
                                                                                    owl:oneOf ( musica:Batteria
                                                                                              )
                                                                                  ]
                                                                                ) ;
                                                             rdf:type owl:Class
                                                           ]
                                      ] ;
                  rdfs:subClassOf musica:Musicista ;
                  rdfs:comment "Musicista che suona uno strumento a percussione, nello specifico la Batteria." ;
                  rdfs:label "Batterista"@it ,
                             "Drummer"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Brano
musica:Brano rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                          owl:onProperty musica:haGenere ;
                                                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass musica:GenereMusicale
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty musica:prodottoDa ;
                                                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass musica:Produttore
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty musica:scrittoDa ;
                                                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass musica:Musicista
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] ;
             rdfs:comment "Composizione vocale scritta per una o più voci perlopiù con accompagnamento strumentale." ;
             rdfs:label "Brano"@it ,
                        "Song"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#CanaleDigitale
musica:CanaleDigitale rdf:type owl:Class ;
                      rdfs:subClassOf musica:CanaleDistribuzione ;
                      owl:disjointUnionOf ( musica:Radio
                                            musica:Streaming
                                            musica:Televisione
                                          ) ;
                      rdfs:comment "Un canale di distribuzione digitale è un mezzo che utilizza la tecnologia elettronica per la distribuzione di beni e servizi." ;
                      rdfs:label "Canale Digitale"@it ,
                                 "Digital Channel"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#CanaleDistribuzione
musica:CanaleDistribuzione rdf:type owl:Class ;
                           owl:equivalentClass [ rdf:type owl:Restriction ;
                                                 owl:onProperty musica:distribuisce ;
                                                 owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                 owl:onClass [ rdf:type owl:Class ;
                                                               owl:unionOf ( musica:Album
                                                                             musica:Brano
                                                                           )
                                                             ]
                                               ] ;
                           rdfs:comment "La distribuzione commerciale è lo strumento attraverso il quale le aziende produttrici e distributrici immettono sul mercato beni e servizi." ;
                           rdfs:label "Canale di Distribuzione"@it ,
                                      "Distribution Channel"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#CanaleFisico
musica:CanaleFisico rdf:type owl:Class ;
                    rdfs:subClassOf musica:CanaleDistribuzione ;
                    rdfs:comment "Un canale di distribuzione fisico è un mezzo che utilizza supporti materiali per la distribuzione di beni e servizi." ;
                    rdfs:label "Canale Fisico"@it ,
                               "Physical Channel"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Cantante
musica:Cantante rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty musica:suona ;
                                      owl:someValuesFrom [ rdf:type owl:Class ;
                                                           owl:oneOf ( musica:Voce
                                                                     )
                                                         ]
                                    ] ;
                rdfs:subClassOf musica:Musicista ;
                rdfs:comment "Cantante"@it ,
                             "Musicista che, mediante l'utilizzo della propria voce, produce una successione di suoni ordinati per ritmo e altezza." ;
                rdfs:label "Singer"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#CasaDiscografica
musica:CasaDiscografica rdf:type owl:Class ;
                        owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                     owl:onProperty <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember> ;
                                                                     owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                     owl:onClass musica:Musicista
                                                                   ]
                                                                   [ rdf:type owl:Restriction ;
                                                                     owl:onProperty <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember> ;
                                                                     owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                     owl:onClass musica:Produttore
                                                                   ]
                                                                 ) ;
                                              rdf:type owl:Class
                                            ] ;
                        rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection> ;
                        rdfs:comment "Attività di produzione e vendita di musica." ,
                                     "Business producing and selling music."@en ;
                        rdfs:label "Casa Discografica"@it ,
                                   "Record Company"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Chitarrista
musica:Chitarrista rdf:type owl:Class ;
                   owl:equivalentClass [ rdf:type owl:Restriction ;
                                         owl:onProperty musica:suona ;
                                         owl:someValuesFrom [ owl:intersectionOf ( musica:StrumentoACorde
                                                                                   [ rdf:type owl:Class ;
                                                                                     owl:oneOf ( musica:ChitarraClassica
                                                                                                 musica:ChitarraElettrica
                                                                                                 musica:ChitarraRitmica
                                                                                               )
                                                                                   ]
                                                                                 ) ;
                                                              rdf:type owl:Class
                                                            ]
                                       ] ;
                   rdfs:subClassOf musica:Musicista ;
                   rdfs:comment "Musicista che suona uno strumento a corde, nello specifico la chitarra classica o la chitarra elettrica." ;
                   rdfs:label "Chitarrista"@it ,
                              "Guitarist"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Città
musica:Città rdf:type owl:Class ;
             rdfs:subClassOf musica:Luogo ;
             rdfs:comment "Large permanent human settlement."@en ;
             rdfs:label "Città"@it ,
                        "City"@en ;
             rdfs:seeAlso "https://www.wikidata.org/wiki/Q515" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Concerto
musica:Concerto rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty musica:numeroSpettatori ;
                                      owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                           owl:onDatatype xsd:unsignedInt ;
                                                           owl:withRestrictions ( [ xsd:minExclusive "500"^^xsd:unsignedInt
                                                                                  ]
                                                                                  [ xsd:maxInclusive "10000"^^xsd:unsignedInt
                                                                                  ]
                                                                                )
