@prefix : <http://www.semanticweb.org/lorenzo/ontologies/music#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/lorenzo/ontologies/musica> .

<http://www.semanticweb.org/lorenzo/ontologies/musica> rdf:type owl:Ontology ;
                                                        owl:imports <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl> ,
                                                                    <http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl> .

#################################################################
#    Object Properties
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf
<http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf> owl:inverseOf :hasMember ;
                                                                               rdfs:label "membro di" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#albumHaGenere
:albumHaGenere rdf:type owl:ObjectProperty ;
               rdfs:domain :Album ;
               rdfs:range :GenereMusicale ;
               owl:propertyChainAxiom ( :contiene
                                        :haGenere
                                      ) ;
               rdfs:label "ha genere" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#assisteA
:assisteA rdf:type owl:ObjectProperty ;
          rdfs:domain :Persona ;
          rdfs:range :Evento ;
          rdfs:label "assiste a" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#compostoDa
:compostoDa rdf:type owl:ObjectProperty ;
            owl:inverseOf :haComposto ;
            rdfs:domain :Brano ;
            rdfs:range :Musicista ;
            rdfs:label "composto da" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#contenutoIn
:contenutoIn rdf:type owl:ObjectProperty ;
             owl:inverseOf :contiene ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain :Brano ;
             rdfs:range :Album ;
             rdfs:label "contenuto in" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#contiene
:contiene rdf:type owl:ObjectProperty ;
          rdfs:domain :Album ;
          rdfs:range :Brano ;
          rdfs:label "contiene" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#distribuisce
:distribuisce rdf:type owl:ObjectProperty ;
              owl:inverseOf :distribuitoTramite ;
              rdfs:domain :CanaleDistribuzione ;
              rdfs:range :Brano ;
              rdfs:label "distribuisce" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#distribuitoTramite
:distribuitoTramite rdf:type owl:ObjectProperty ;
                    rdfs:domain :Brano ;
                    rdfs:range :CanaleDistribuzione ;
                    rdfs:label "distribuito tramite" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#genereDelBrano
:genereDelBrano rdf:type owl:ObjectProperty ;
                owl:inverseOf :haGenere ;
                rdfs:domain :GenereMusicale ;
                rdfs:range :Brano ;
                rdfs:label "genere di" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#haAlbum
:haAlbum rdf:type owl:ObjectProperty ;
         rdfs:domain :Musicista ;
         rdfs:range :Album ;
         owl:propertyChainAxiom ( :haComposto
                                  :contenutoIn
                                ) ;
         rdfs:label "ha album" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#haComposto
:haComposto rdf:type owl:ObjectProperty ;
            rdfs:domain :Musicista ;
            rdfs:range :Brano ;
            rdfs:label "ha composto" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#haGenere
:haGenere rdf:type owl:ObjectProperty ;
          rdfs:domain :Brano ;
          rdfs:range :GenereMusicale ;
          rdfs:label "ha genere" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#hasMember
:hasMember rdf:type owl:ObjectProperty ;
           rdfs:range owl:Thing ;
           rdfs:comment "A relation between collections and entities, e.g. 'my collection of saxophones includes an old Adolphe Sax original alto' (i.e. my collection has member an Adolphe Sax alto)."@en ;
           rdfs:isDefinedBy "http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl"@en ;
           rdfs:label "ha membro" ,
                      "has member"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/music#ospita
:ospita rdf:type owl:ObjectProperty ;
        owl:inverseOf :ospiteDi ;
        rdfs:domain :Evento ;
        rdfs:range [ rdf:type owl:Class ;
                     owl:unionOf ( :Band
                                   :Musicista
                                 )
                   ] ;
        rdfs:label "ospita" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#ospiteDi
:ospiteDi rdf:type owl:ObjectProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :Band
                                      :Musicista
                                    )
                      ] ;
          rdfs:range :Evento ;
          rdfs:label "ospite di" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#situatoIn
:situatoIn rdf:type owl:ObjectProperty ,
                    owl:TransitiveProperty ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :Evento
                                       :Luogo
                                     )
                       ] ;
           rdfs:range :Luogo ;
           rdfs:label "situato in" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#suona
:suona rdf:type owl:ObjectProperty ;
       owl:inverseOf :suonatoDa ;
       rdfs:domain :Musicista ;
       rdfs:range :Strumento ;
       rdfs:label "suona" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#suonatoDa
:suonatoDa rdf:type owl:ObjectProperty ;
           rdfs:domain :Strumento ;
           rdfs:range :Musicista ;
           rdfs:label "suonato da" .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/lorenzo/ontologies/music#dataEvento
:dataEvento rdf:type owl:DatatypeProperty ;
            rdfs:domain :Evento ;
            rdfs:range xsd:dateTime ;
            rdfs:label "data evento" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#dataUscita
:dataUscita rdf:type owl:DatatypeProperty ;
            rdfs:domain :Album ;
            rdfs:range xsd:dateTime ;
            rdfs:label "data uscita" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#età
:età rdf:type owl:DatatypeProperty ;
     rdfs:domain :Persona ;
     rdfs:range xsd:unsignedInt ;
     rdfs:label "età" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#numeroSpettatori
:numeroSpettatori rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Evento ;
                  rdfs:range xsd:unsignedInt ;
                  rdfs:label "numero spettatori" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#numeroVendite
:numeroVendite rdf:type owl:DatatypeProperty ;
               rdfs:domain :Album ;
               rdfs:range xsd:unsignedInt ;
               rdfs:label "numero vendite" .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/lorenzo/ontologies/music#Adulto
:Adulto rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Restriction ;
                              owl:onProperty :età ;
                              owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                   owl:onDatatype xsd:unsignedInt ;
                                                   owl:withRestrictions ( [ xsd:minInclusive "30"^^xsd:unsignedInt
                                                                          ]
                                                                          [ xsd:maxExclusive "65"^^xsd:unsignedInt
                                                                          ]
                                                                        )
                                                 ]
                            ] ;
        rdfs:subClassOf :Generazione ;
        rdfs:comment "Persona che ha raggiunto il pieno sviluppo fisico e psichico di età compresa tra 30 e 65 anni." ;
        rdfs:label "Adulto" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Album
:Album rdf:type owl:Class ;
       owl:equivalentClass [ rdf:type owl:Restriction ;
                             owl:onProperty :contiene ;
                             owl:minQualifiedCardinality "3"^^xsd:nonNegativeInteger ;
                             owl:onClass :Brano
                           ] ;
       rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#Object> ;
       rdfs:comment "Raccolta di brani musicali di uno stesso artista." ;
       rdfs:label "Album" ;
       rdfs:seeAlso "https://www.wikidata.org/wiki/Q482994" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#AlbumDiamante
:AlbumDiamante rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty :numeroVendite ;
                                     owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                          owl:onDatatype xsd:unsignedInt ;
                                                          owl:withRestrictions ( [ xsd:minInclusive "1000000"^^xsd:unsignedInt
                                                                                 ]
                                                                               )
                                                        ]
                                   ] ;
               rdfs:subClassOf :Album ;
               rdfs:comment "Album musicale che ha totalizzato un numero di vendite pari ad almeno 1 milione." ;
               rdfs:label "Album di Diamante" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#AlbumOro
:AlbumOro rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty :numeroVendite ;
                                owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                     owl:onDatatype xsd:unsignedInt ;
                                                     owl:withRestrictions ( [ xsd:minExclusive "50000"^^xsd:unsignedInt
                                                                            ]
                                                                            [ xsd:maxExclusive "100000"^^xsd:unsignedInt
                                                                            ]
                                                                          )
                                                   ]
                              ] ;
          rdfs:subClassOf :Album ;
          rdfs:comment "Album musicale che ha totalizzato un numero di vendite compreso tra 50 mila e 100 mila." ;
          rdfs:label "Album d'Oro" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#AlbumPlatino
:AlbumPlatino rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Restriction ;
                                    owl:onProperty :numeroVendite ;
                                    owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                         owl:onDatatype xsd:unsignedInt ;
                                                         owl:withRestrictions ( [ xsd:minExclusive "100000"^^xsd:unsignedInt
                                                                                ]
                                                                                [ xsd:maxExclusive "1000000"^^xsd:unsignedInt
                                                                                ]
                                                                              )
                                                       ]
                                  ] ;
              rdfs:subClassOf :Album ;
              rdfs:comment "Album musicale che ha totalizzato un numero di vendite compreso tra 100 mila e 1 milione." ;
              rdfs:label "Album di Platino" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Anziano
:Anziano rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Restriction ;
                               owl:onProperty :età ;
                               owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                    owl:onDatatype xsd:unsignedInt ;
                                                    owl:withRestrictions ( [ xsd:minInclusive "65"^^xsd:unsignedInt
                                                                           ]
                                                                         )
                                                  ]
                             ] ;
         rdfs:subClassOf :Generazione ;
         rdfs:comment "Persona di età uguale o superiore a 65 anni." ;
         rdfs:label "Anziano" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Band
:Band rdf:type owl:Class ;
      owl:equivalentClass [ rdf:type owl:Restriction ;
                            owl:onProperty :hasMember ;
                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :Musicista
                          ] ;
      rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection> ;
      rdfs:comment "Gruppo di musicisti." ;
      rdfs:label "Band" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Bassista
:Bassista rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty :suona ;
                                owl:someValuesFrom [ owl:intersectionOf ( :StrumentoACorde
                                                                          [ rdf:type owl:Class ;
                                                                            owl:oneOf ( :Basso
                                                                                      )
                                                                          ]
                                                                        ) ;
                                                     rdf:type owl:Class
                                                   ]
                              ] ;
          rdfs:subClassOf :Musicista ;
          rdfs:comment "Musicista che suona uno strumento a corde, nello specifico il Basso." ;
          rdfs:label "Bassista" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Batterista
:Batterista rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty :suona ;
                                  owl:someValuesFrom [ owl:intersectionOf ( :StrumentoAPercussione
                                                                            [ rdf:type owl:Class ;
                                                                              owl:oneOf ( :Batteria
                                                                                        )
                                                                            ]
                                                                          ) ;
                                                       rdf:type owl:Class
                                                     ]
                                ] ;
            rdfs:subClassOf :Musicista ;
            rdfs:comment "Musicista che suona uno strumento a percussione, nello specifico la Batteria." ;
            rdfs:label "Batterista" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Brano
:Brano rdf:type owl:Class ;
       owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                    owl:onProperty :haGenere ;
                                                    owl:someValuesFrom :GenereMusicale
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :compostoDa ;
                                                    owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                    owl:onClass :Musicista
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] ;
       rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#Object> ;
       rdfs:comment "Composizione vocale scritta per una o più voci perlopiù con accompagnamento strumentale." ;
       rdfs:label "Brano" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#CanaleDigitale
:CanaleDigitale rdf:type owl:Class ;
                rdfs:subClassOf :CanaleDistribuzione ;
                owl:disjointUnionOf ( :Radio
                                      :Streaming
                                      :Televisione
                                    ) ;
                rdfs:comment "Un canale di distribuzione digitale è un mezzo che utilizza la tecnologia elettronica per la distribuzione di beni e servizi." ;
                rdfs:label "Canale Digitale" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#CanaleDistribuzione
:CanaleDistribuzione rdf:type owl:Class ;
                     rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#Object> ;
                     rdfs:comment "La distribuzione commerciale è lo strumento attraverso il quale le aziende produttrici e distributrici immettono sul mercato beni e servizi." ;
                     rdfs:label "Canale di Distribuzione" ;
                     rdfs:seeAlso "https://dbpedia.org/page/Distribution_(marketing)" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#CanaleFisico
:CanaleFisico rdf:type owl:Class ;
              rdfs:subClassOf :CanaleDistribuzione ;
              rdfs:comment "Un canale di distribuzione fisico è un mezzo che utilizza supporti materiali per la distribuzione di beni e servizi." ;
              rdfs:label "Canale Fisico" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Cantante
:Cantante rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty :suona ;
                                owl:someValuesFrom [ rdf:type owl:Class ;
                                                     owl:oneOf ( :Voce
                                                               )
                                                   ]
                              ] ;
          rdfs:subClassOf :Musicista ;
          rdfs:comment "Cantante" ,
                       "Musicista che, mediante l'utilizzo della propria voce, produce una successione di suoni ordinati per ritmo e altezza." .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Chitarrista
:Chitarrista rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Restriction ;
                                   owl:onProperty :suona ;
                                   owl:someValuesFrom [ owl:intersectionOf ( :StrumentoACorde
                                                                             [ rdf:type owl:Class ;
                                                                               owl:oneOf ( :ChitarraClassica
                                                                                           :ChitarraElettrica
                                                                                           :ChitarraRitmica
                                                                                         )
                                                                             ]
                                                                           ) ;
                                                        rdf:type owl:Class
                                                      ]
                                 ] ;
             rdfs:subClassOf :Musicista ;
             rdfs:comment "Musicista che suona uno strumento a corde, nello specifico la chitarra classica o la chitarra elettrica." ;
             rdfs:label "Chitarrista" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Città
:Città rdf:type owl:Class ;
       rdfs:subClassOf :Luogo ;
       rdfs:comment "Large permanent human settlement."@en ;
       rdfs:label "Città" ,
                  "City"@en ;
       rdfs:seeAlso "https://www.wikidata.org/wiki/Q515" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Concerto
:Concerto rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty :numeroSpettatori ;
                                owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                     owl:onDatatype xsd:unsignedInt ;
                                                     owl:withRestrictions ( [ xsd:minExclusive "500"^^xsd:unsignedInt
                                                                            ]
                                                                            [ xsd:maxInclusive "10000"^^xsd:unsignedInt
                                                                            ]
                                                                          )
                                                   ]
                              ] ;
          rdfs:subClassOf :Evento ;
          rdfs:comment "Evento musicale a cui partecipa un numero di spettatori compreso tra 500 e 10 mila." ;
          rdfs:label "Concerto" ;
          rdfs:seeAlso "https://www.wikidata.org/wiki/Q182832" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Continente
:Continente rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Class ;
                                  owl:oneOf ( :Africa
                                              :AmericaDelNord
                                              :AmericaDelSud
                                              :Asia
                                              :Europa
                                              :Oceania
                                            )
                                ] ;
            rdfs:subClassOf :Luogo ;
            rdfs:comment "Large landmass identified by convention."@en ;
            rdfs:label "Continent"@en ,
                       "Continente" ;
            rdfs:seeAlso "https://www.wikidata.org/wiki/Q5107" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Esibizione
:Esibizione rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty :numeroSpettatori ;
                                  owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                       owl:onDatatype xsd:unsignedInt ;
                                                       owl:withRestrictions ( [ xsd:minInclusive "10"^^xsd:unsignedInt
                                                                              ]
                                                                              [ xsd:maxExclusive "500"^^xsd:unsignedInt
                                                                              ]
                                                                            )
                                                     ]
                                ] ;
            rdfs:subClassOf :Evento ;
            rdfs:comment "Evento musicale a cui partecipa un numero di spettatori compreso tra 10 e 500." ;
            rdfs:label "Esibizione" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Evento
:Evento rdf:type owl:Class ;
        rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#Object> ;
        owl:disjointUnionOf ( :Concerto
                              :Esibizione
                              :Festival
                            ) ;
        rdfs:comment "Eventi temporanei e programmati, come una conferenza, un festival, una competizione o simili." ;
        rdfs:label "Evento" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Festival
:Festival rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty :numeroSpettatori ;
                                owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                     owl:onDatatype xsd:unsignedInt ;
                                                     owl:withRestrictions ( [ xsd:minExclusive "10000"^^xsd:unsignedInt
                                                                            ]
                                                                          )
                                                   ]
                              ] ;
          rdfs:subClassOf :Evento ;
          rdfs:comment "Evento musicale a cui partecipa un numero di spettatori maggiore di 10 mila." ;
          rdfs:label "Festival" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Generazione
:Generazione rdf:type owl:Class ;
             rdfs:subClassOf :Persona ;
             owl:disjointUnionOf ( :Adulto
                                   :Anziano
                                   :Giovane
                                 ) ;
             rdfs:comment "A generation refers to all of the people born and living at about the same time, regarded collectively. It can also be described as, \"the average period, generally considered to be about 20–⁠30 years, during which children are born and grow up, become adults, and begin to have children.\" In kinship terminology, it is a structural term designating the parent-child relationship. It is known as biogenesis, reproduction, or procreation in the biological sciences."@en ;
             rdfs:label "Generation"@en ,
                        "Generazione" ;
             rdfs:seeAlso "https://dbpedia.org/page/Generation" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#GenereMusicale
:GenereMusicale rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Class ;
                                      owl:oneOf ( :Folk
                                                  :Jazz
                                                  :Pop
                                                  :Rock
                                                )
                                    ] ;
                rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#Object> ;
                rdfs:comment "Categoria convenzionale che identifica e classifica i brani e le composizioni in base a criteri di affinità." ;
                rdfs:label "Genere Musicale" ;
                rdfs:seeAlso "https://dbpedia.org/ontology/MusicGenre" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Giovane
:Giovane rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Restriction ;
                               owl:onProperty :età ;
                               owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                    owl:onDatatype xsd:unsignedInt ;
                                                    owl:withRestrictions ( [ xsd:minInclusive "12"^^xsd:unsignedInt
                                                                           ]
                                                                           [ xsd:maxExclusive "30"^^xsd:unsignedInt
                                                                           ]
                                                                         )
                                                  ]
                             ] ;
         rdfs:subClassOf :Generazione ;
         rdfs:comment "Persona di età giovanile compresa tra 12 e 30 anni." ;
         rdfs:label "Giovane" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Luogo
:Luogo rdf:type owl:Class ;
       rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#Object> ;
       rdfs:comment "Location of the object, structure or event."@en ;
       rdfs:label "Location"@en ,
                  "Luogo" ;
       rdfs:seeAlso "https://www.wikidata.org/wiki/Property:P276" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Musicista
:Musicista rdf:type owl:Class ;
           owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                        owl:onProperty :haComposto ;
                                                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                        owl:onClass :Brano
                                                      ]
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :suona ;
                                                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                        owl:onClass :Strumento
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ;
           rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#Role> ;
           rdfs:comment "Il musicista è una persona che svolge e ha talento per una determinata attività in musica, inerente alla creazione o all'esecuzione di composizioni strumentali, vocali, corali." ;
           rdfs:label "Musicista" .


###  http://www.semanticweb.org/lorenzo/ontologies/music#Nazione
:Nazione rdf:type owl:Class ;
         rdfs:subClassOf :Luogo ;
