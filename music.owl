@prefix : <http://www.semanticweb.org/lorenzo/ontologies/music#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/lorenzo/ontologies/musica> .

<http://www.semanticweb.org/lorenzo/ontologies/musica> rdf:type owl:Ontology .

#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/lorenzo/ontologies/musica#albumHaGenere
:albumHaGenere rdf:type owl:ObjectProperty ;
               rdfs:domain :Album ;
               rdfs:range :GenereMusicale ;
               owl:propertyChainAxiom ( :contiene
                                        :haGenere
                                      ) .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#assisteA
:assisteA rdf:type owl:ObjectProperty ;
          rdfs:domain :Persona ;
          rdfs:range :Evento .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#certifica
:certifica rdf:type owl:ObjectProperty ;
           owl:inverseOf :èCertificato ;
           rdfs:range :Album .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#compostoDa
:compostoDa rdf:type owl:ObjectProperty ;
            owl:inverseOf :haComposto ;
            rdfs:domain :Brano ;
            rdfs:range :Musicista .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#contenutoIn
:contenutoIn rdf:type owl:ObjectProperty ;
             owl:inverseOf :contiene ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain :Brano ;
             rdfs:range :Album .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#contiene
:contiene rdf:type owl:ObjectProperty ;
          rdfs:domain :Album ;
          rdfs:range :Brano .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#distribuisce
:distribuisce rdf:type owl:ObjectProperty ;
              owl:inverseOf :distribuitoTramite ;
              rdfs:domain :PiattaformaDistribuzione ;
              rdfs:range :Brano .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#distribuitoTramite
:distribuitoTramite rdf:type owl:ObjectProperty ;
                    rdfs:domain :Brano ;
                    rdfs:range :PiattaformaDistribuzione .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#genereDelBrano
:genereDelBrano rdf:type owl:ObjectProperty ;
                owl:inverseOf :haGenere ;
                rdfs:domain :GenereMusicale ;
                rdfs:range :Brano .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#haAlbum
:haAlbum rdf:type owl:ObjectProperty ;
         rdfs:domain :Musicista ;
         rdfs:range :Album ;
         owl:propertyChainAxiom ( :haComposto
                                  :contenutoIn
                                ) .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#haComposto
:haComposto rdf:type owl:ObjectProperty ;
            rdfs:domain :Musicista ;
            rdfs:range :Brano .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#haGenere
:haGenere rdf:type owl:ObjectProperty ;
          rdfs:domain :Brano ;
          rdfs:range :GenereMusicale .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#hasMember
:hasMember rdf:type owl:ObjectProperty ;
           owl:inverseOf :isMemberOf ;
           rdfs:domain :Collection ;
           rdfs:range owl:Thing ;
           rdfs:comment "A relation between collections and entities, e.g. 'my collection of saxophones includes an old Adolphe Sax original alto' (i.e. my collection has member an Adolphe Sax alto)."@en ;
           rdfs:isDefinedBy "http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl"@en ;
           rdfs:label "haMembro" ,
                      "has member"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#isMemberOf
:isMemberOf rdf:type owl:ObjectProperty ;
            rdfs:domain owl:Thing ;
            rdfs:range :Collection ;
            rdfs:label "èMembroDi" .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#iscrittoA
:iscrittoA rdf:type owl:ObjectProperty ;
           rdfs:domain :Persona ;
           rdfs:range :PiattaformaDistribuzione .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#ospita
:ospita rdf:type owl:ObjectProperty ;
        owl:inverseOf :ospiteDi ;
        rdfs:domain :Evento ;
        rdfs:range :Musicista .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#ospiteDi
:ospiteDi rdf:type owl:ObjectProperty ;
          rdfs:domain :Musicista ;
          rdfs:range :Evento .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#situatoIn
:situatoIn rdf:type owl:ObjectProperty ,
                    owl:TransitiveProperty ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :Evento
                                       :Luogo
                                     )
                       ] ;
           rdfs:range :Luogo .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#suona
:suona rdf:type owl:ObjectProperty ;
       owl:inverseOf :suonatoDa ;
       rdfs:domain :Musicista ;
       rdfs:range :Strumento .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#suonatoDa
:suonatoDa rdf:type owl:ObjectProperty ;
           rdfs:domain :Strumento ;
           rdfs:range :Musicista .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#èCertificato
:èCertificato rdf:type owl:ObjectProperty ;
              rdfs:domain :Album .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/lorenzo/ontologies/musica#dataEvento
:dataEvento rdf:type owl:DatatypeProperty .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#dataUscita
:dataUscita rdf:type owl:DatatypeProperty ;
            rdfs:domain :Album ;
            rdfs:range xsd:dateTime .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#età
:età rdf:type owl:DatatypeProperty ;
     rdfs:domain :Persona ;
     rdfs:range xsd:unsignedInt .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#numeroSpettatori
:numeroSpettatori rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Evento ;
                  rdfs:range xsd:unsignedInt .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#numeroVendite
:numeroVendite rdf:type owl:DatatypeProperty ;
               rdfs:domain :Album ;
               rdfs:range xsd:unsignedInt .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/lorenzo/ontologies/musica#Adulto
:Adulto rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Restriction ;
                              owl:onProperty :età ;
                              owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                   owl:onDatatype xsd:unsignedInt ;
                                                   owl:withRestrictions ( [ xsd:minInclusive "30"^^xsd:unsignedInt
                                                                          ]
                                                                          [ xsd:maxInclusive "60"^^xsd:unsignedInt
                                                                          ]
                                                                        )
                                                 ]
                            ] ;
        rdfs:subClassOf :Generazione .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#Album
:Album rdf:type owl:Class ;
       owl:equivalentClass [ rdf:type owl:Restriction ;
                             owl:onProperty :contiene ;
                             owl:minQualifiedCardinality "3"^^xsd:nonNegativeInteger ;
                             owl:onClass :Brano
                           ] ;
       rdfs:comment "Raccolta di brani musicali di uno stesso artista" ;
       rdfs:label "Album" ;
       rdfs:seeAlso "https://www.wikidata.org/wiki/Q482994" .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#AlbumDiamante
:AlbumDiamante rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty :numeroVendite ;
                                     owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                          owl:onDatatype xsd:unsignedInt ;
                                                          owl:withRestrictions ( [ xsd:minInclusive "1000000"^^xsd:unsignedInt
                                                                                 ]
                                                                               )
                                                        ]
                                   ] ;
               rdfs:subClassOf :Album .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#AlbumOro
:AlbumOro rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty :numeroVendite ;
                                owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                     owl:onDatatype xsd:unsignedInt ;
                                                     owl:withRestrictions ( [ xsd:minExclusive "50000"^^xsd:unsignedInt
                                                                            ]
                                                                            [ xsd:maxExclusive "100000"^^xsd:unsignedInt
                                                                            ]
                                                                          )
                                                   ]
                              ] ;
          rdfs:subClassOf :Album .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#AlbumPlatino
:AlbumPlatino rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Restriction ;
                                    owl:onProperty :numeroVendite ;
                                    owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                         owl:onDatatype xsd:unsignedInt ;
                                                         owl:withRestrictions ( [ xsd:minExclusive "100000"^^xsd:unsignedInt
                                                                                ]
                                                                                [ xsd:maxExclusive "1000000"^^xsd:unsignedInt
                                                                                ]
                                                                              )
                                                       ]
                                  ] ;
              rdfs:subClassOf :Album .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#Anziano
:Anziano rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Restriction ;
                               owl:onProperty :età ;
                               owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                    owl:onDatatype xsd:unsignedInt ;
                                                    owl:withRestrictions ( [ xsd:minExclusive "60"^^xsd:unsignedInt
                                                                           ]
                                                                         )
                                                  ]
                             ] ;
         rdfs:subClassOf :Generazione .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#Band
:Band rdf:type owl:Class ;
      owl:equivalentClass [ rdf:type owl:Restriction ;
                            owl:onProperty :hasMember ;
                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :Musicista
                          ] ;
      rdfs:subClassOf :Collection .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#Bassista
:Bassista rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty :suona ;
                                owl:someValuesFrom [ owl:intersectionOf ( :StrumentoACorde
                                                                          [ rdf:type owl:Class ;
                                                                            owl:oneOf ( :Basso
                                                                                      )
                                                                          ]
                                                                        ) ;
                                                     rdf:type owl:Class
                                                   ]
                              ] ;
          rdfs:subClassOf :Musicista .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#Batterista
:Batterista rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty :suona ;
                                  owl:someValuesFrom [ owl:intersectionOf ( :StrumentoAPercussione
                                                                            [ rdf:type owl:Class ;
                                                                              owl:oneOf ( :Batteria
                                                                                        )
                                                                            ]
                                                                          ) ;
                                                       rdf:type owl:Class
                                                     ]
                                ] ;
            rdfs:subClassOf :Musicista .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#Brano
:Brano rdf:type owl:Class ;
       owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                    owl:onProperty :haGenere ;
                                                    owl:someValuesFrom :GenereMusicale
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :compostoDa ;
                                                    owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                    owl:onClass :Musicista
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#Cantante
:Cantante rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty :suona ;
                                owl:someValuesFrom [ rdf:type owl:Class ;
                                                     owl:oneOf ( :Voce
                                                               )
                                                   ]
                              ] ;
          rdfs:subClassOf :Musicista .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#Chitarrista
:Chitarrista rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Restriction ;
                                   owl:onProperty :suona ;
                                   owl:someValuesFrom [ owl:intersectionOf ( :StrumentoACorde
                                                                             [ rdf:type owl:Class ;
                                                                               owl:oneOf ( :ChitarraClassica
                                                                                           :ChitarraElettrica
                                                                                         )
                                                                             ]
                                                                           ) ;
                                                        rdf:type owl:Class
                                                      ]
                                 ] ;
             rdfs:subClassOf :Musicista .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#Città
:Città rdf:type owl:Class ;
       rdfs:subClassOf :Luogo .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#Collection
:Collection rdf:type owl:Class ;
            rdfs:isDefinedBy "http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl" ;
            rdfs:label "Collection"@en .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#Concerto
:Concerto rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty :numeroSpettatori ;
                                owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                     owl:onDatatype xsd:unsignedInt ;
                                                     owl:withRestrictions ( [ xsd:minExclusive "500"^^xsd:unsignedInt
                                                                            ]
                                                                            [ xsd:maxInclusive "10000"^^xsd:unsignedInt
                                                                            ]
                                                                          )
                                                   ]
                              ] ;
          rdfs:subClassOf :Evento .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#Continente
:Continente rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Class ;
                                  owl:oneOf ( :Africa
                                              :AmericaDelNord
                                              :AmericaDelSud
                                              :Asia
                                              :Europa
                                              :Oceania
                                            )
                                ] ;
            rdfs:subClassOf :Luogo .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#Esibizione
:Esibizione rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty :numeroSpettatori ;
                                  owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                       owl:onDatatype xsd:unsignedInt ;
                                                       owl:withRestrictions ( [ xsd:minInclusive "10"^^xsd:unsignedInt
                                                                              ]
                                                                              [ xsd:maxExclusive "500"^^xsd:unsignedInt
                                                                              ]
                                                                            )
                                                     ]
                                ] ;
            rdfs:subClassOf :Evento .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#Evento
:Evento rdf:type owl:Class ;
        owl:disjointUnionOf ( :Concerto
                              :Esibizione
                              :Festival
                            ) .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#Festival
:Festival rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty :numeroSpettatori ;
                                owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                     owl:onDatatype xsd:unsignedInt ;
                                                     owl:withRestrictions ( [ xsd:minExclusive "10000"^^xsd:unsignedInt
                                                                            ]
                                                                          )
                                                   ]
                              ] ;
          rdfs:subClassOf :Evento .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#Generazione
:Generazione rdf:type owl:Class ;
             rdfs:subClassOf :Persona ;
             owl:disjointUnionOf ( :Adulto
                                   :Anziano
                                   :Giovane
                                 ) .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#GenereMusicale
:GenereMusicale rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Class ;
                                      owl:oneOf ( :Folk
                                                  :Jazz
                                                  :Pop
                                                  :Rock
                                                )
                                    ] ;
                rdfs:comment "Categoria convenzionale che identifica e classifica i brani e le composizioni in base a criteri di affinità." ;
                rdfs:label "Genere Musicale" ;
                rdfs:seeAlso "https://dbpedia.org/ontology/MusicGenre" .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#Giovane
:Giovane rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Restriction ;
                               owl:onProperty :età ;
                               owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                    owl:onDatatype xsd:unsignedInt ;
                                                    owl:withRestrictions ( [ xsd:minInclusive "12"^^xsd:unsignedInt
                                                                           ]
                                                                           [ xsd:maxExclusive "30"^^xsd:unsignedInt
                                                                           ]
                                                                         )
                                                  ]
                             ] ;
         rdfs:subClassOf :Generazione .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#Luogo
:Luogo rdf:type owl:Class .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#Musicista
:Musicista rdf:type owl:Class ;
           owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                        owl:onProperty :haComposto ;
                                                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                        owl:onClass :Brano
                                                      ]
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :suona ;
                                                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                        owl:onClass :Strumento
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ;
           rdfs:subClassOf :Ruolo .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#Nazione
:Nazione rdf:type owl:Class ;
         rdfs:subClassOf :Luogo .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#Negozio
:Negozio rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Class ;
                               owl:oneOf ( :EsseStore
                                           :MediaWorld
                                           :Wallmart
                                         )
                             ] ;
         rdfs:subClassOf :PiattaformaAnalogica .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#Persona
:Persona rdf:type owl:Class .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#PiattaformaAnalogica
:PiattaformaAnalogica rdf:type owl:Class ;
                      rdfs:subClassOf :PiattaformaDistribuzione .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#PiattaformaDigitale
:PiattaformaDigitale rdf:type owl:Class ;
                     rdfs:subClassOf :PiattaformaDistribuzione ;
                     owl:disjointUnionOf ( :Radio
                                           :Streaming
                                           :Televisione
                                         ) .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#PiattaformaDistribuzione
:PiattaformaDistribuzione rdf:type owl:Class .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#Radio
:Radio rdf:type owl:Class ;
       owl:equivalentClass [ rdf:type owl:Class ;
                             owl:oneOf ( :DiscoRadio
                                         :RDS
                                         :RTL102.5
                                         :RadioZeta
                                       )
                           ] ;
       rdfs:subClassOf :PiattaformaDigitale .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#Ruolo
:Ruolo rdf:type owl:Class ;
       rdfs:subClassOf :Persona .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#Spettatore
:Spettatore rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty :assisteA ;
                                  owl:someValuesFrom :Evento
                                ] ;
            rdfs:subClassOf :Ruolo .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#Streaming
:Streaming rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Class ;
                                 owl:oneOf ( :AppleMusic
                                             :SoundCloud
                                             :Spotify
                                             :Vevo
                                             :Youtube
                                           )
                               ] ;
           rdfs:subClassOf :PiattaformaDigitale .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#Strumento
:Strumento rdf:type owl:Class ;
           owl:disjointUnionOf ( :StrumentoACorde
                                 :StrumentoAFiato
                                 :StrumentoAPercussione
                                 :StrumentoATastiera
                               ) .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#StrumentoACorde
:StrumentoACorde rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Class ;
                                       owl:oneOf ( :Arpa
                                                   :Basso
                                                   :ChitarraClassica
                                                   :ChitarraElettrica
                                                   :Violino
                                                   :Violoncello
                                                 )
                                     ] ;
                 rdfs:subClassOf :Strumento .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#StrumentoAFiato
:StrumentoAFiato rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Class ;
                                       owl:oneOf ( :Clarinetto
                                                   :Flauto
                                                   :Microfono
                                                   :Sassofono
                                                   :Trombone
                                                 )
                                     ] ;
                 rdfs:subClassOf :Strumento .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#StrumentoAPercussione
:StrumentoAPercussione rdf:type owl:Class ;
                       owl:equivalentClass [ rdf:type owl:Class ;
                                             owl:oneOf ( :Batteria
                                                       )
                                           ] ;
                       rdfs:subClassOf :Strumento .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#StrumentoATastiera
:StrumentoATastiera rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Class ;
                                          owl:oneOf ( :Organo
                                                      :Pianoforte
                                                      :Pianola
                                                    )
                                        ] ;
                    rdfs:subClassOf :Strumento .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#Tastierista
:Tastierista rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Restriction ;
                                   owl:onProperty :suona ;
                                   owl:someValuesFrom :StrumentoATastiera
                                 ] ;
             rdfs:subClassOf :Musicista .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#Televisione
:Televisione rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Class ;
                                   owl:oneOf ( :KissTV
                                               :MTV
                                               <http://www.semanticweb.org/lorenzo/ontologies/musica#4Music>
                                             )
                                 ] ;
             rdfs:subClassOf :PiattaformaDigitale .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#Utente
:Utente rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Restriction ;
                              owl:onProperty :iscrittoA ;
                              owl:someValuesFrom :PiattaformaDistribuzione
                            ] ;
        rdfs:subClassOf :Ruolo .


#################################################################
#    Individuals
#################################################################

###  http://www.semanticweb.org/lorenzo/ontologies/musica#Africa
:Africa rdf:type owl:NamedIndividual .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#AmericaDelNord
:AmericaDelNord rdf:type owl:NamedIndividual .


###  http://www.semanticweb.org/lorenzo/ontologies/musica#AmericaDelSud
:AmericaDelSud rdf:type owl:NamedIndividual .


